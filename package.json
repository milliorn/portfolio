{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "private": "true",
  "description": "A personal portfolio website built with Astro, showcasing projects, blog posts, and contact information",
  "repository": "https://github.com/milliorn/portfolio",
  "author": "Scott Milliorn",
  "license": "GPL-3.0-only",
  "keywords": [
    "astro",
    "astrojs",
    "static-site",
    "portfolio",
    "personal-portfolio",
    "developer-portfolio",
    "web-development",
    "frontend",
    "ssg",
    "netlify",
    "typescript",
    "javascript",
    "css",
    "blog",
    "resume"
  ],
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "astro check",
    "dev": "astro dev",
    "docs": "astro docs",
    "help": "astro --help",
    "info": "astro info",
    "preview": "astro preview",
    "sync": "astro sync",
    "verbose": "astro dev --verbose",
    "version": "astro version",
    "telemetry": "astro telemetry",
    "format": "prettier --write .",
    "git-update": "git fetch && git pull",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "lint:watch": "esw -w --clear --ext .js,.jsx,.ts,.tsx .",
    "update": "npm update && npm install"
  },
  "dependencies": {
    "@astrojs/netlify": "^6.2.5",
    "astro": "^5.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-watch": "^8.0.0"
