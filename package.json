{
  "name": "portfolio",
  "engines": {
    "node": ">=18.14.1"
  },
  "author": "Scott Milliorn",
  "description": "A personal portfolio website built with Astro, showcasing projects, blog posts, and contact information",
  "license": "GPL-3.0",
  "private": "true",
  "repository": "https://github.com/milliorn/portfolio",
  "type": "module",
  "version": "0.0.1",
  "keywords": [
    "astro",
    "astrojs",
    "static-site",
    "portfolio",
    "personal-portfolio",
    "developer-portfolio",
    "web-development",
    "frontend",
    "ssg",
    "netlify",
    "typescript",
    "javascript",
    "css",
    "blog",
    "resume"
  ],
  "scripts": {
    "astro": "astro",
    "astro:build": "astro build",
    "astro:check": "astro check",
    "astro:dev": "astro dev",
    "astro:docs": "astro docs",
    "astro:help": "astro --help",
    "astro:info": "astro info",
    "astro:preview": "astro preview",
    "astro:sync": "astro sync",
    "astro:telemetry": "astro telemetry",
    "astro:verbose": "astro dev --verbose",
    "astro:version": "astro version",
    "lint:check": "npm run astro:check && npm run eslint:check && npm run prettier:check && npm run tsc:check",
    "lint:fix": "npm run prettier:write && npm run eslint:fix",
    "eslint:check": "eslint \"src/**/*.{ts,tsx,astro}\"",
    "eslint:fix": "eslint \"src/**/*.{ts,tsx,astro}\" --fix",
    "eslint:watch": "esw -w --clear --ext .js,.jsx,.ts,.tsx,.astro src",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier --write .",
    "tsc:check": "tsc --noEmit",
    "tsc:watch": "tsc --watch"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^6.4.0",
    "@astrojs/sitemap": "^3.4.2",
    "astro": "^5.10.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vite-pwa/astro": "^1.1.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-watch": "^8.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.8.3"
  }
}
